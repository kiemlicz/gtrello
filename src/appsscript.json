    "gmail": {
      "name": "GTrello",
      "logoUrl": "https://raw.githubusercontent.com/google/material-design-icons/master/png/action/view_kanban/materialicons/24dp/2x/baseline_view_kanban_black_24dp.png",
      "contextualTriggers": [],
      "homepageTrigger": {
        "enabled": true,
        "runFunction": "mainApp"
